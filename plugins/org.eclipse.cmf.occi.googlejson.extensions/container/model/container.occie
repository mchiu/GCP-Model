      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="preemptible" required="true" description="Whether the nodes are created as preemptible VM instances. See:&#xA;https://cloud.google.com/compute/docs/instances/preemptible for more&#xA;information about preemptible VM instances.">
      <type xsi:type="occi:BooleanType" href="http://schemas.ogf.org/occi/core#//@types.1"/>
