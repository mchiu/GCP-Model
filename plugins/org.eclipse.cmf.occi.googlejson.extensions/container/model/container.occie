      <type xsi:type="occi:NumericType" href="http://schemas.ogf.org/occi/core#//@types.2"/>
    </attributes>
    <attributes name="loggingService" required="true" description="The logging service the cluster should use to write logs.&#xA;Currently available options:&#xA;&#xA;* `logging.googleapis.com` - the Google Cloud Logging service.&#xA;* `none` - no logs will be exported from the cluster.&#xA;* if left as an empty string,`logging.googleapis.com` will be used.">
      <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
    </attributes>
    <attributes name="statusMessage" required="true" description="[Output only] Additional information about the current status of this&#xA;cluster, if available.">
