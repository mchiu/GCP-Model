    <attributes name="syntax" required="true" description="The source syntax." type="//@types.13"/>
    <attributes name="name" required="true" description="Enum type name." type="//@types.13"/>
    <attributes name="enumvalue" required="true" description="Enum value definitions." type="//@types.7"/>
