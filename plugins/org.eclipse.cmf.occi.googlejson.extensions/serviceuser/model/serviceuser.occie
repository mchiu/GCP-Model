  </kinds>
  <kinds name="Backend" scheme="http://occi/gcpserviceuser#" title="`Backend` defines the backend configuration for a service.">
    <attributes name="rules" required="true" description="A list of API backend rules that apply to individual API methods.&#xA;&#xA;**NOTE:** All service configuration rules follow &quot;last one wins&quot; order." type="//@types.7"/>
