    <attributes name="downloadService" required="true" description="DO NOT USE FIELDS BELOW THIS LINE UNTIL THIS WARNING IS REMOVED.&#xA;&#xA;Specify name of the download service if one is used for download." type="//@types.17"/>
    <attributes name="completeNotification" required="true" description="A boolean that determines whether a notification for the completion of a&#xA;download should be sent to the backend." type="//@types.6"/>
    <attributes name="maxDirectDownloadSize" required="true" description="Optional maximum acceptable size for direct download.&#xA;The size is specified in bytes." type="//@types.17"/>
    <attributes name="dropzone" required="true" description="Name of the Scotty dropzone to use for the current API." type="//@types.17"/>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
