    <literals name="NOT_SUPPORTED" documentation="Permission is not supported for custom role use."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="Stage" documentation="The current launch stage of the permission.">
    <literals name="ALPHA" documentation="The permission is currently in an alpha phase."/>
    <literals name="BETA" documentation="The permission is currently in a beta phase."/>
    <literals name="GA" documentation="The permission is generally available."/>
    <literals name="DEPRECATED" documentation="The permission is being deprecated."/>
