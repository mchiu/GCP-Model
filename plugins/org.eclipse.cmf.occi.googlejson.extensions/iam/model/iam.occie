    <literals name="BETA" documentation="The user has indicated this role is currently in a beta phase."/>
    <literals name="GA" documentation="The user has indicated this role is generally available."/>
    <literals name="DEPRECATED" documentation="The user has indicated this role is being deprecated."/>
    <literals name="DISABLED" documentation="This role is disabled and will not contribute permissions to any members&#xA;it is granted to in policies."/>
    <literals name="EAP" documentation="The user has indicated this role is currently in an eap phase."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="View" documentation="">
