    <literals name="GENERIC" documentation="A `GENERIC` annotation type should be used when no other annotation&#xA;type will suffice. This represents an untyped annotation of the reference&#xA;genome."/>
    <literals name="VARIANT" documentation="A `VARIANT` annotation type."/>
    <literals name="GENE" documentation="A `GENE` annotation type represents the existence of a gene at the&#xA;associated reference coordinates. The start coordinate is typically the&#xA;gene's transcription start site and the end is typically the end of the&#xA;gene's last exon."/>
    <literals name="TRANSCRIPT" documentation="A `TRANSCRIPT` annotation type represents the assertion that a&#xA;particular region of the reference genome may be transcribed as RNA."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="VariantSetMetadataTtype" documentation="The type of data. Possible types include: Integer, Float,&#xA;Flag, Character, and String.">
    <literals name="TYPE_UNSPECIFIED" documentation=""/>
    <literals name="INTEGER" documentation=""/>
    <literals name="FLOAT" documentation=""/>
    <literals name="FLAG" documentation=""/>
    <literals name="CHARACTER" documentation=""/>
    <literals name="STRING" documentation=""/>
  </types>
  <types xsi:type="occi:EnumerationType" name="infoMergeConfig" documentation="A mapping between info field keys and the InfoMergeOperations to&#xA;be performed on them.">
    <literals name="INFO_MERGE_OPERATION_UNSPECIFIED" documentation=""/>
    <literals name="IGNORE_NEW" documentation=""/>
    <literals name="MOVE_TO_CALLS" documentation=""/>
  </types>
  <types xsi:type="occi:EnumerationType" name="operation" documentation="">
    <literals name="OPERATION_UNSPECIFIED" documentation=""/>
    <literals name="ALIGNMENT_MATCH" documentation="An alignment match indicates that a sequence can be aligned to the&#xA;reference without evidence of an INDEL. Unlike the&#xA;`SEQUENCE_MATCH` and `SEQUENCE_MISMATCH` operators,&#xA;the `ALIGNMENT_MATCH` operator does not indicate whether the&#xA;reference and read sequences are an exact match. This operator is&#xA;equivalent to SAM's `M`."/>
    <literals name="INSERT" documentation="The insert operator indicates that the read contains evidence of bases&#xA;being inserted into the reference. This operator is equivalent to SAM's&#xA;`I`."/>
    <literals name="DELETE" documentation="The delete operator indicates that the read contains evidence of bases&#xA;being deleted from the reference. This operator is equivalent to SAM's&#xA;`D`."/>
    <literals name="SKIP" documentation="The skip operator indicates that this read skips a long segment of the&#xA;reference, but the bases have not been deleted. This operator is commonly&#xA;used when working with RNA-seq data, where reads may skip long segments&#xA;of the reference between exons. This operator is equivalent to SAM's&#xA;`N`."/>
    <literals name="CLIP_SOFT" documentation="The soft clip operator indicates that bases at the start/end of a read&#xA;have not been considered during alignment. This may occur if the majority&#xA;of a read maps, except for low quality bases at the start/end of a read.&#xA;This operator is equivalent to SAM's `S`. Bases that are soft&#xA;clipped will still be stored in the read."/>
    <literals name="CLIP_HARD" documentation="The hard clip operator indicates that bases at the start/end of a read&#xA;have been omitted from this alignment. This may occur if this linear&#xA;alignment is part of a chimeric alignment, or if the read has been&#xA;trimmed (for example, during error correction or to trim poly-A tails for&#xA;RNA-seq). This operator is equivalent to SAM's `H`."/>
    <literals name="PAD" documentation="The pad operator indicates that there is padding in an alignment. This&#xA;operator is equivalent to SAM's `P`."/>
    <literals name="SEQUENCE_MATCH" documentation="This operator indicates that this portion of the aligned sequence exactly&#xA;matches the reference. This operator is equivalent to SAM's `=`."/>
    <literals name="SEQUENCE_MISMATCH" documentation="This operator indicates that this portion of the aligned sequence is an&#xA;alignment match to the reference, but a sequence mismatch. This can&#xA;indicate a SNP or a read error. This operator is equivalent to SAM's&#xA;`X`."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="AnnotationSetTtype" documentation="The type of annotations contained within this set.">
    <literals name="ANNOTATION_TYPE_UNSPECIFIED" documentation=""/>
    <literals name="GENERIC" documentation="A `GENERIC` annotation type should be used when no other annotation&#xA;type will suffice. This represents an untyped annotation of the reference&#xA;genome."/>
    <literals name="VARIANT" documentation="A `VARIANT` annotation type."/>
    <literals name="GENE" documentation="A `GENE` annotation type represents the existence of a gene at the&#xA;associated reference coordinates. The start coordinate is typically the&#xA;gene's transcription start site and the end is typically the end of the&#xA;gene's last exon."/>
    <literals name="TRANSCRIPT" documentation="A `TRANSCRIPT` annotation type represents the assertion that a&#xA;particular region of the reference genome may be transcribed as RNA."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="ImportVariantsRequestIinfomergeconfig" documentation="A mapping between info field keys and the InfoMergeOperations to&#xA;be performed on them. This is plumbed down to the MergeVariantRequests&#xA;generated by the resulting import job.">
    <literals name="INFO_MERGE_OPERATION_UNSPECIFIED" documentation=""/>
    <literals name="IGNORE_NEW" documentation=""/>
    <literals name="MOVE_TO_CALLS" documentation=""/>
  </types>
  <types xsi:type="occi:EnumerationType" name="format" documentation="The format of the variant data being imported. If unspecified, defaults to&#xA;to `VCF`.">
    <literals name="FORMAT_UNSPECIFIED" documentation=""/>
    <literals name="FORMAT_VCF" documentation="VCF (Variant Call Format). The VCF files may be gzip compressed. gVCF is&#xA;also supported. Disclaimer: gzip VCF imports are currently much slower&#xA;than equivalent uncompressed VCF imports. For this reason, uncompressed&#xA;VCF is currently recommended for imports with more than 1GB combined&#xA;uncompressed size, or for time sensitive imports."/>
    <literals name="FORMAT_COMPLETE_GENOMICS" documentation="Complete Genomics masterVarBeta format. The masterVarBeta files may&#xA;be bzip2 compressed."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="effect" documentation="Effect of the variant on the coding sequence.">
    <literals name="EFFECT_UNSPECIFIED" documentation=""/>
    <literals name="EFFECT_OTHER" documentation="`EFFECT_OTHER` should be used when no other Effect&#xA;will suffice."/>
    <literals name="FRAMESHIFT" documentation="`FRAMESHIFT` indicates a mutation in which the insertion or&#xA;deletion of nucleotides resulted in a frameshift change."/>
    <literals name="FRAME_PRESERVING_INDEL" documentation="`FRAME_PRESERVING_INDEL` indicates a mutation in which a&#xA;multiple of three nucleotides has been inserted or deleted, resulting&#xA;in no change to the reading frame of the coding sequence."/>
    <literals name="SYNONYMOUS_SNP" documentation="`SYNONYMOUS_SNP` indicates a single nucleotide polymorphism&#xA;mutation that results in no amino acid change."/>
    <literals name="NONSYNONYMOUS_SNP" documentation="`NONSYNONYMOUS_SNP` indicates a single nucleotide&#xA;polymorphism mutation that results in an amino acid change."/>
    <literals name="STOP_GAIN" documentation="`STOP_GAIN` indicates a mutation that leads to the creation&#xA;of a stop codon at the variant site. Frameshift mutations creating&#xA;downstream stop codons do not count as `STOP_GAIN`."/>
    <literals name="STOP_LOSS" documentation="`STOP_LOSS` indicates a mutation that eliminates a&#xA;stop codon at the variant site."/>
    <literals name="SPLICE_SITE_DISRUPTION" documentation="`SPLICE_SITE_DISRUPTION` indicates that this variant is&#xA;found in a splice site for the associated transcript, and alters the&#xA;normal splicing pattern."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="VariantAnnotationTtype" documentation="Type has been adapted from ClinVar's list of variant types.">
    <literals name="TYPE_UNSPECIFIED" documentation=""/>
    <literals name="TYPE_OTHER" documentation="`TYPE_OTHER` should be used when no other Type will suffice.&#xA;Further explanation of the variant type may be included in the&#xA;info field."/>
    <literals name="INSERTION" documentation="`INSERTION` indicates an insertion."/>
    <literals name="DELETION" documentation="`DELETION` indicates a deletion."/>
    <literals name="SUBSTITUTION" documentation="`SUBSTITUTION` indicates a block substitution of&#xA;two or more nucleotides."/>
    <literals name="SNP" documentation="`SNP` indicates a single nucleotide polymorphism."/>
    <literals name="STRUCTURAL" documentation="`STRUCTURAL` indicates a large structural variant,&#xA;including chromosomal fusions, inversions, etc."/>
    <literals name="CNV" documentation="`CNV` indicates a variation in copy number."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="clinicalSignificance" documentation="Describes the clinical significance of a variant.&#xA;It is adapted from the ClinVar controlled vocabulary for clinical&#xA;significance described at:&#xA;http://www.ncbi.nlm.nih.gov/clinvar/docs/clinsig/">
    <literals name="CLINICAL_SIGNIFICANCE_UNSPECIFIED" documentation=""/>
    <literals name="CLINICAL_SIGNIFICANCE_OTHER" documentation="`OTHER` should be used when no other clinical significance&#xA;value will suffice."/>
    <literals name="UNCERTAIN" documentation=""/>
    <literals name="BENIGN" documentation=""/>
    <literals name="LIKELY_BENIGN" documentation=""/>
    <literals name="LIKELY_PATHOGENIC" documentation=""/>
    <literals name="PATHOGENIC" documentation=""/>
    <literals name="DRUG_RESPONSE" documentation=""/>
    <literals name="HISTOCOMPATIBILITY" documentation=""/>
    <literals name="CONFERS_SENSITIVITY" documentation=""/>
    <literals name="RISK_FACTOR" documentation=""/>
    <literals name="ASSOCIATION" documentation=""/>
    <literals name="PROTECTIVE" documentation=""/>
    <literals name="MULTIPLE_REPORTED" documentation="`MULTIPLE_REPORTED` should be used when multiple clinical&#xA;signficances are reported for a variant. The original clinical&#xA;significance values may be provided in the `info` field."/>
  </types>
  <types xsi:type="occi:EnumerationType" name="ExportVariantSetRequestFformat" documentation="The format for the exported data.">
    <literals name="FORMAT_UNSPECIFIED" documentation=""/>
    <literals name="FORMAT_BIGQUERY" documentation="Export the data to Google BigQuery."/>
  </types>
</occi:Extension>
