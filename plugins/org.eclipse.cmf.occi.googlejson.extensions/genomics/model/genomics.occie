<occi:Extension xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:occi="http://schemas.ogf.org/occi/core/ecore" name="genomics" scheme="http://occi/gcpgenomics#" description="Upload, process, query, and search Genomics data in the cloud.">
  <import href="http://schemas.ogf.org/occi/core#/"/>
  <kinds name="ExperimentLink" scheme="http://occi/gcpgenomics#" title="The experiment used to generate this read group." source="//@kinds[term='readgroup']" target="//@kinds[term='experiment']">
