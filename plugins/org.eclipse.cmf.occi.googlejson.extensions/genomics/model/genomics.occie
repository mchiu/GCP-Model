  <kinds name="CigarUnit" scheme="http://occi/gcpgenomics#" title="A single CIGAR operation.">
    <attributes name="referenceSequence" required="true" description="`referenceSequence` is only used at mismatches&#xA;(`SEQUENCE_MISMATCH`) and deletions (`DELETE`).&#xA;Filling this field replaces SAM's MD tag. If the relevant information is&#xA;not available, this field is unset." type="//@types.13"/>
