  <kinds name="ExprLink" scheme="http://occi/gcpdeploymentmanager#" title="The condition that is associated with this binding. NOTE: an unsatisfied condition will not allow user access via current binding. Different bindings, including their conditions, are examined independently. This field is GOOGLE_INTERNAL." source="//@kinds[term='binding']" target="//@kinds[term='expr']">
