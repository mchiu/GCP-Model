    </actions>
    <actions name="delete" scheme="http://occi/gcpdeploymentmanager/deployment/action#" title="Deletes a deployment and all of the resources in the deployment.">
      <attributes name="deletePolicy" required="true" description="Sets the policy to use for deleting resources.">
        <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
