      </attributes>
    </actions>
    <actions name="update" scheme="http://occi/gcpdeploymentmanager/deployment/action#" title="Updates a deployment and all of the resources described by the deployment manifest.">
      <attributes name="createPolicy" required="true" description="Sets the policy to use for creating new resources.">
        <type xsi:type="occi:StringType" href="http://schemas.ogf.org/occi/core#//@types.0"/>
      </attributes>
      <attributes name="deletePolicy" required="true" description="Sets the policy to use for deleting resources.">
