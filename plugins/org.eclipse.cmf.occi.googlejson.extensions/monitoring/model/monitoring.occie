  <kinds name="UptimeCheckConfig" scheme="http://occi/gcpmonitoring#" title="This message configures which resources and services to monitor for availability.">
    <attributes name="timeout" required="true" description="The maximum amount of time to wait for the request to complete (must be between 1 and 60 seconds). Required." type="//@types.13"/>
    <attributes name="period" required="true" description="How often the uptime check is performed. Currently, only 1, 5, 10, and 15 minutes are supported. Required." type="//@types.13"/>
    <attributes name="name" required="true" description="A unique resource name for this UptimeCheckConfig. The format is:projects/[PROJECT_ID]/uptimeCheckConfigs/[UPTIME_CHECK_ID].This field should be omitted when creating the uptime check configuration; on create, the resource name is assigned by the server and included in the response." type="//@types.13"/>
    <attributes name="selectedRegions" required="true" description="The list of regions from which the check will be run. If this field is specified, enough regions to include a minimum of 3 locations must be provided, or an error message is returned. Not specifying this field will result in uptime checks running from all regions." type="//@types.7"/>
    <attributes name="displayName" required="true" description="A human-friendly name for the uptime check configuration. The display name should be unique within a Stackdriver Account in order to make it easier to identify; however, uniqueness is not enforced. Required." type="//@types.13"/>
    <attributes name="contentMatchers" required="true" description="The expected content on the page the check is run against. Currently, only the first entry in the list is supported, and other entries will be ignored. The server will look for an exact match of the string in the page response's content. This field is optional and should only be specified if a content match is required." type="//@types.7"/>
    <actions name="list" scheme="http://occi/gcpmonitoring/uptimecheckconfig/action#" title="Lists the existing valid uptime check configurations for the project, leaving out any invalid configurations.">
      <attributes name="pageToken" required="true" description="If this field is not empty then it must contain the nextPageToken value returned by a previous call to this method. Using this field causes the method to return more results from the previous method call." type="//@types.13"/>
      <attributes name="pageSize" required="true" description="The maximum number of results to return in a single response. The server may further constrain the maximum number of results returned in a single page. If the page_size is &lt;=0, the server will decide the number of results to be returned." type="//@types.3"/>
      <attributes name="parent" required="true" description="The project whose uptime check configurations are listed. The format isprojects/[PROJECT_ID]." type="//@types.13"/>
    </actions>
    <actions name="get" scheme="http://occi/gcpmonitoring/uptimecheckconfig/action#" title="Gets a single uptime check configuration.">
      <attributes name="name" required="true" description="The uptime check configuration to retrieve. The format isprojects/[PROJECT_ID]/uptimeCheckConfigs/[UPTIME_CHECK_ID]." type="//@types.13"/>
    </actions>
    <actions name="patch" scheme="http://occi/gcpmonitoring/uptimecheckconfig/action#" title="Updates an uptime check configuration. You can either replace the entire configuration with a new one or replace only certain fields in the current configuration by specifying the fields to be updated via &quot;updateMask&quot;. Returns the updated configuration.">
      <attributes name="updateMask" required="true" description="Optional. If present, only the listed fields in the current uptime check configuration are updated with values from the new configuration. If this field is empty, then the current configuration is completely replaced with the new configuration." type="//@types.13"/>
      <attributes name="name1" required="true" description="The uptime check configuration to update. The format isprojects/[PROJECT_ID]/uptimeCheckConfigs/[UPTIME_CHECK_ID]." type="//@types.13"/>
      <attributes name="name" required="true" description="A unique resource name for this UptimeCheckConfig. The format is:projects/[PROJECT_ID]/uptimeCheckConfigs/[UPTIME_CHECK_ID].This field should be omitted when creating the uptime check configuration; on create, the resource name is assigned by the server and included in the response." type="//@types.13"/>
    </actions>
    <actions name="create" scheme="http://occi/gcpmonitoring/uptimecheckconfig/action#" title="Creates a new uptime check configuration.">
      <attributes name="parent" required="true" description="The project in which to create the uptime check. The format is:projects/[PROJECT_ID]." type="//@types.13"/>
    </actions>
    <actions name="delete" scheme="http://occi/gcpmonitoring/uptimecheckconfig/action#" title="Deletes an uptime check configuration. Note that this method will fail if the uptime check configuration is referenced by an alert policy or other dependent configs that would be rendered invalid by the deletion.">
      <attributes name="name" required="true" description="The uptime check configuration to delete. The format isprojects/[PROJECT_ID]/uptimeCheckConfigs/[UPTIME_CHECK_ID]." type="//@types.13"/>
    </actions>
    <parent href="http://schemas.ogf.org/occi/core#//@kinds[term='resource']"/>
  </kinds>
